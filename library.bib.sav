@inproceedings{Becker2013,
	Abstract = {Within this article the prototype icebricks is described by its main characteristics which are layers of abstraction, attribution, reference models and semantic standardization by the use of a glossary. The layers are predefined in order to enhance the clarity and comparability of the processes. Attribution is used -- beside the layers of abstraction -- as means to reduce the complexity of the models by shifting information to analyzable and easily maintainable attributes instead of sophisticated control flows within the process elements. The glossary is inspired by the idea of model conventions. Each business object within it is defined once and can be used with defined activities as process building blocks within all models with the same semantic meaning. Furthermore, reference models are incorporated into the prototype to enable modelers with the possibility to create or derive purposeful models in a short period of time. The prototype was thoroughly and very successfully evaluated in its web based version in two process modeling projects aiming for process reorganization for an ERP system change and for a complete documentation of the process landscape for knowledge management.},
	Author = {Becker, J{\"{o}}rg and Clever, Nico and Holler, Justus and Shitkova, Maria},
	Booktitle = {Lecture Notes in Computer Science (including subseries Lecture Notes in Artificial Intelligence and Lecture Notes in Bioinformatics)},
	Date-Added = {2017-01-27 13:46:04 +0000},
	Date-Modified = {2017-01-27 13:46:04 +0000},
	Doi = {10.1007/978-3-642-38827-9_27},
	Isbn = {9783642388262},
	Issn = {03029743},
	Keywords = {Building Block based Modeling,Business Process Management,Modeling Languages,Modeling Tools,Prototype},
	Pages = {394--399},
	Title = {{Icebricks: Business process modeling on the basis of semantic standardization}},
	Volume = {7939 LNCS},
	Year = {2013},
	}

@InCollection{Becker2007,
  author        = {Becker, J{\"{o}}rg and Delfmann, Patrick and Knackstedt, Ralf},
  title         = {{Adaptive reference modeling: Integrating configurative and generic adaptation techniques for information models}},
  booktitle     = {Reference Modeling: Efficient Information Systems Design Through Reuse of Information Models},
  publisher     = {Springer Nature},
  year          = {2007},
  pages         = {27--58},
  isbn          = {9783790819656},
  abstract      = {Reference models have to be adapted to fit to the according application situation. In order to reduce the adaptation efforts, the concept of configurative reference modeling represents a promising approach. Nevertheless, since not every requirement of possible reference model users can be anticipated by the reference model developer, further model adaptations have to be performed. In order to support the reference model user decreasing his adaptation efforts by providing a higher methodological support, we propose to integrate generic model adaptation techniques with configurative reference modeling. Our paper presents recommendations for the construction of modeling languages that realize an integration of configurative and generic reference modeling.},
  date-added    = {2017-01-27 13:46:04 +0000},
  date-modified = {2017-01-27 13:46:04 +0000},
  doi           = {10.1007/978-3-7908-1966-3_2},
}

